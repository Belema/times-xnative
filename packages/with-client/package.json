{
  "name": "@thetimes/with-client",
  "main": "lib/with-client.js",
  "version": "1.0.9",
  "license": "BSD-3-Clause",
  "scripts": {
    "build:types": "mkdir -p lib && for x in src/*.js; do cp -- \"$x\" \"lib/$(basename $x).flow\"; done",
    "build:flow": "babel src -d lib/",
    "build": "yarn build:types && yarn build:flow",
    "watch": "babel src -d lib/ --watch",
    "fmt": "prettier --write 'src/**/*.*'",
    "lint:flow": "flow",
    "lint:prettier": "prettier --list-different src/**/*.*",
    "lint": "yarn lint:prettier && yarn lint:flow"
  },
  "dependencies": {
    "@times-components/utils": "0.8.0",
    "apollo-cache-inmemory": "1.1.5",
    "apollo-client": "2.2.3",
    "apollo-link-http": "1.5.3",
    "graphql": "0.12.3",
    "react": "16.3.1",
    "react-apollo": "2.1.0-rc.3",
    "react-native": "0.54.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "flow-bin": "0.63.1",
    "prettier": "1.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
