{
  "name": "@thetimes/ios-app",
  "version": "0.0.0",
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "build:js": "haul bundle --platform ios --dev false --entry-file js/index.ios.js --bundle-output assets/js/index.ios.bundle --assets-dest assets/res/",
    "build:meta": "cat package.json | grep version | head -1 | sed 's/[\",\t ]//g' | awk -F: '{ print \"Bundle Version: \" $2 }' > assets/js/index.ios.bundle.meta",
    "build": "yarn build:js && yarn build:meta",
    "fmt": "prettier --write 'js/**/*.*'",
    "start": "haul start --platform ios",
    "lint:prettier": "prettier --list-different js/**/*.*",
    "lint": "yarn lint:prettier"
  },
  "dependencies": {
    "@thetimes/article": "1.0.17",
    "@thetimes/author-profile": "1.0.12",
    "@thetimes/topic": "0.0.2",
    "react": "16.3.1",
    "react-native": "0.54.2",
    "react-native-svg": "5.5.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-2": "6.24.1",
    "flow-bin": "0.63.1",
    "haul": "^1.0.0-beta.13",
    "prettier": "1.8.2",
    "webpack": "2"
  }
}
