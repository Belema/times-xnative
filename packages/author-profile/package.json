{
  "name": "@thetimes/author-profile",
  "main": "lib/with-client.js",
  "version": "1.0.0",
  "scripts": {
    "build:types": "for x in src/*.js; do cp -- \"$x\" \"lib/$(basename $x).flow\"; done",
    "build:flow": "babel src -d lib/",
    "build": "npm run build:types && npm run build:flow",
    "lint:prettier": "prettier --list-different src/**/*.*",
    "lint": "npm run lint:prettier"
  },
  "dependencies": {
    "@thetimes/with-client": "1.0.0",
    "@times-components/author-profile": "^0.41.21",
    "@times-components/provider": "^0.22.6",
    "react": "^16.2.0",
    "react-native": "^0.53.0"
  }
}
