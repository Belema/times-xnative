{
  "name": "@thetimes/author-profile",
  "main": "lib/with-client.js",
  "version": "1.0.0",
  "scripts": {
    "build:types":
      "for x in src/*.js; do cp -- \"$x\" \"lib/$(basename $x).flow\"; done",
    "build:flow": "babel src -d lib/",
    "build": "npm run build:types && npm run build:flow",
    "fmt": "prettier --write 'src/**/*.*'"
  },
  "dependencies": {
    "@thetimes/with-client": "1.0.0",
    "@times-components/author-profile": "^0.41.21",
    "@times-components/provider": "^0.22.6",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "flow-bin": "^0.65.0",
    "prettier": "^1.10.2"
  }
}
